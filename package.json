{
  "name": "neuro-balance-clone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "gen:sitemap": "node scripts/generate-sitemaps.mjs",
    "build": "npm run gen:sitemap && vite build",
    "deploy": "npm run gen:sitemap && bash scripts/deploy-to-deploy-branch.sh",
    "ping:search": "node -e \"const p=u=>fetch(u).catch(()=>{}); p('https://www.google.com/ping?sitemap=https://neurobalance.co/sitemap-index.xml'); p('https://www.bing.com/ping?sitemap=https://neurobalance.co/sitemap-index.xml'); console.log('Pinged search engines');\"",
    "postdeploy": "npm run ping:search"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.8",
    "@react-three/fiber": "^9.1.2",
    "classnames": "^2.5.1",
    "daisyui": "^5.0.46",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.14.0",
    "html-react-parser": "^5.2.5",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-awesome-reveal": "^4.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.3",
    "react-scroll": "^1.9.3",
    "react-scroll-parallax": "^3.4.5",
    "styled-components": "^6.1.19",
    "sweetalert2": "^11.22.5",
    "swiper": "^11.2.10",
    "swr": "^2.3.4",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5"
  }
}
